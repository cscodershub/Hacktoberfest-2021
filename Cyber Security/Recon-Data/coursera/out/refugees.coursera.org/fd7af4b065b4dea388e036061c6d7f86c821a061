	function closePromo() {
		console.log("here");
	jQuery(".coursera-promo-bar").animate({
		height: 0,
		padding: 0
	}, 750, function() {
		jQuery(".coursera-promo-bar").css('display', 'none');
	});
	
	sessionStorage.setItem("Coursera-Refugee-Promo", "true");
}
jQuery(document).ready(function($) {
	var hgt=54;
	if(parseInt(jQuery(window).width())<=480) {
		hgt=72;
	}
	if (sessionStorage.getItem("Coursera-Refugee-Promo") == "true") {
		$(".coursera-promo-bar").css("display", "none");
	} else {
		$(".coursera-promo-bar").css("display", "block");
		$(".coursera-promo-bar").css("height", hgt+"px");
		
	}
});
	
