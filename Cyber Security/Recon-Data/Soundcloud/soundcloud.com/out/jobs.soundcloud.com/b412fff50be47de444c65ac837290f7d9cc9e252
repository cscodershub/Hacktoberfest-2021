	function iCopyAnalyticsEvent( name, properties )
	{
		var params_copy = window.__analytics_called_parameters.slice();
		var data = {};
		properties = properties || {};

		for( i in params_copy[ 2 ] )
		{
			if( params_copy[ 2 ].hasOwnProperty( i ) )
			{
				data[ i ] = params_copy[ 2 ][ i ];
			}
		}

		for( i in properties )
		{
			if( properties.hasOwnProperty( i ) )
			{
				data[ i ] = properties[ i ];
			}
		}

		if( name === 'conversion' && window.__unique )
		{
			if( window.__unique.isVisited() )
			{
				data[ 'responsive_mode' ] = window.__unique.isResponsiveMode() ? 'mobile' : null;
			}
		}


		params_copy.splice( 1, 2, name, data );
		return params_copy;
	}

	function removeEventParameter( name )
	{
		if( window.__analytics_called_parameters ) {
			var parameters = window.__analytics_called_parameters[2] ? window.__analytics_called_parameters[2] : {};

			if( parameters[name] )
			{
				delete parameters[name];
				window.__analytics_called_parameters[2] = parameters;
			}
		}
	}

	function iEncodePixelUrl( id, event, data )
	{
		var endpoint = "anthill.instapage.com" + '/api/v3/projects/' + id + '/events/' + event;
		var base64 = window.base64_encode( JSON.stringify( data ) );
		var base64_url_safe = base64.replace(/\+/g, '-').replace(/\//g, '_');
		var jwt = 'eyJ0eXAiOiJKV1QiLCJhbGciOiJSUzI1NiIsImtpZCI6IjEifQ.eyJpc3MiOiJhcHAiLCJzdWIiOiJmcm9udGVuZDphcHA6dHAxNTM3MjQ2MCIsImF1ZCI6WyJhbnRoaWxsIl0sImV4cCI6MTYzMzYzNjcxOX0.bVx95ngEZGVHITbYIZ0X0dZsOsoAkvqpdefuguNbaWaZEfDUzYbgEMxGVz-JUM_q8HGCdfOLaU0mxyS3oqB41rAn5oyjuCiwhN2HwNlOvFuikMO3Tvl14PdELDkFeUjbkOmK-2Mct1SfzvIeOw6n6ZY96edT4p6ikkuHNC7wUy3SeNoAktP1oNqkfnugoEjM5WyvkIi7dWzRc9mdvMn0ciZMbmB0UvXtUNQ5VK-JMf5-tAqqcWa4JznDVXsIDYMR3l1uKmLKEHFQ4k3_0sQwEYZyqh5EAU1Xp7fOcg7gn2zpe07FDRWTzUiBgEuA7asfleaYfjcWipEC39pjkngm-w';
		var query = 'data=' + base64_url_safe + '&t=' + Date.now() + '&jwt=' + jwt;
		var image_url = '//' + endpoint + '?' + query;

		return image_url;
	}

	function iCreateTrackingPixel( src )
	{
		return ijQuery( '<img>')
			.attr( {
				src: src,
				width: 1,
				height: 1,
				alt: '',
				title: '',
				style: 'display: table-cell; height: 0; position: absolute; left: -9999999px; top: -999999px;'
			} )
	}

	ijQuery( document ).on( 'ready', function()
	{
		var cookie_name = 'instapage-visit-15372460';
		var parameters = {"owner_id":2750096,"customer_id":3251671,"user_id":2750096,"page_id":15372460,"published_version":63,"quantity":1,"static_page":false,"variation_name":"A","variation_id":1,"linked_variation_id":2,"initial_responsive_mode":null,"visitor_ip":"10.99.14.5","useragent":"Go-http-client\/1.1"};
		var id = '15372460';
		var track_returning_visitors = true;

		var project_id = '56c2f3d796773d0a7e96a536';
		var event = 'visit';

		parameters.javascript = true;
		parameters.variation = parameters.variation_name;
		parameters.generation_time = '5';
		parameters.responsive_mode = null;

		if( window.__mobile_version === 2 && window.innerWidth < 600 && window.__variant.indexOf( '-mobile' ) < 0 )
		{
			window.__variant += '-mobile';
			parameters.responsive_mode = 'mobile';
		}
		else if( window.__variant.indexOf( '-mobile' ) > 0 )
		{
			parameters.responsive_mode = 'mobile';
		}

		window.__unique = new InstapageUniqueVisit( cookie_name, {
			variant: parameters.variation_name,
			responsive_mode: parameters.responsive_mode,
			reset: {}		} );

		parameters.visited = window.__unique.isVisited();
		parameters.useragent = window.navigator && window.navigator.userAgent || parameters.useragent;

		parameters.campaign_id = window.__unique.getCampaignId();
		parameters.ad_id = window.__unique.getAdId();
		parameters.campaign_source = window.__unique.getCampaignSource();

		parameters.ref = window.__unique.getRef();

		window.__analytics_called_parameters = [ project_id, event, parameters ];
		if( !parameters.visited || track_returning_visitors )
		{
			iCreateTrackingPixel( iEncodePixelUrl( project_id, event, parameters ) )
				.appendTo( ijQuery( 'body' ) );

			window.__unique.setResponsiveMode( parameters.responsive_mode );
			window.__unique.setVisited();
			window.__unique.save();

			//Remove ad_id parameter for multiple submit protection
			removeEventParameter( 'ad_id' );
		}

		if( window.__conversions_settings.external && typeof $.cookie( 'instapage.conversion' + id ) !== 'undefined' )
		{
			$.removeCookie( 'instapage.conversion' + id, {
				path: '/'
			} );
		}
	} );

</script>
