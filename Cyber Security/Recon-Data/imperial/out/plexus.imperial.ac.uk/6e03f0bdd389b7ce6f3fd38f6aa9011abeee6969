<script>
  $(function() {
    // Video
    $video = $('video');
    if ($video.length != 0) $video[0].play();
  
    // Slider
    $('.rslides').responsiveSlides({
      auto : true,
      speed: 2500,
      timeout: 6500,
      pager: AlumniConnect.Helpers.Global.isMobile.any() == null,
      nav: true
    });
    // Slider swipe on mobile
    var slider = document.getElementById('js-swipe-slider');
    var sliderHammer = new Hammer(slider);
    sliderHammer.on('swipeleft', function(e) {
      $('.rslides_nav.next').trigger('click');
    });
    sliderHammer.on('swiperight', function(e) {
      $('.rslides_nav.prev').trigger('click');
    });
  })
</script>
<script>
  $(function() {
    $('.cover-bloc-item__image').beautifullCoverSize();
  
    // Map
    var $map_container = $("#network_map_container");
    var zoom = 2.0;
    if (zoom < 3) zoom = 3;
    var height = $(window).height() / 2.4 || 400;
    if (height < 400) height = 400;
  
    if (!$map_container.length == 0 && AlumniConnect.Helpers.Global.isMobile.any() == null) {
      var MapComponent = React.createElement(MapboxMap, {
        center: {
          lat: 51.498121,
          lng: -0.17727
        },
        enable_zoom: false,
        light_style: true,
        markersUrl: "/user_markers",
        zoom: zoom || undefined,
        height: height,
      });
  
      // send logout message to mobile device
      if ((AlumniConnect.Helpers.Global.getUrlParameter(location.href).from === 'logout') && ('mobileBridge' in window)) {
        window.mobileBridge.jsMessage('logout');
      }
  
      ReactDOM.render(MapComponent, $('#network_map_container')[0]);
    }
  });
</script>
<script>
  $(function() {
    var initialScrollEvent = true;
    var headerSocials      = $('.header-container--socials');
    var headerNav          = $('.header-container--nav');
    var headerNotif        = $('.homepage-notif');
  
    // Header socials bar
    if (headerSocials.length > 0) {
      if ($(window).scrollTop() <= 150) {
        headerNotif.addClass('homepage-notif--top-shifted');
      }
  
      $(window).scroll(function() {
        if (!initialScrollEvent) {
          var winTop = $(this).scrollTop();
          if (winTop >= 150) {
            headerNotif.removeClass('homepage-notif--top-shifted');
            headerSocials.addClass('sticky-socials');
            headerNav.removeClass('header-container--top-shifted');
          } else {
            headerNotif.addClass('homepage-notif--top-shifted');
            headerSocials.removeClass('sticky-socials');
            headerNav.addClass('header-container--top-shifted');
          }
        } else {
          initialScrollEvent = false;
        }
      });
    }
  
    $('.js-toggle-mobile-menu').click(function() {
      $(this).find('i').toggleClass('icon-plus icon-bars');
      $('body').toggleClass('scroll-desactivated')
      $('.js-mobile-menu').toggleClass('open');
    });
  
    $('.js-sub_menu_toggle').click(function() {
      $(this).parents('li').toggleClass('toggle__sub-menu--open');
    });
  
    // Menu wrapped if too long
    var windowWidth = 0;
    var logoContainer = $('.js-logo-container');
    var menuContainer = $('.js-menu-container');
    var menu = $('.js-menu');
    var buttonsRightContainer = $('.js-buttons-right-container');
    var dropdownMoreMenu = $('.js-dropdown-more-menu');
    var homepageContainer = $('.homepage-container');
    var realWidth = 0;
  
    var wrapMenu = function() {
      windowWidth = $(window).width() > homepageContainer.width() ? homepageContainer.width() : $(window).width();
  
      // If an element is missing, avoid
      if (windowWidth && (logoContainer.length > 0) && (menuContainer.length > 0) && (menu.length > 0) && (buttonsRightContainer.length > 0) && (dropdownMoreMenu.length > 0)) {
        realWidth = logoContainer.width() + menuContainer.width() + buttonsRightContainer.width();
  
        if (windowWidth < realWidth) {
          moveRequiredLi();
          dropdownMoreMenu.removeClass('hidden');
        } else {
          dropdownMoreMenu.addClass('hidden');
          menuReveal();
        }
      } else {
        // Reset menu opacity if avoid
        menuReveal();
      }
    };
  
    var menuReveal = function() {
      if (menuContainer.length > 0 && buttonsRightContainer.length > 0) {
        menuContainer.css('opacity', 1);
        buttonsRightContainer.css('opacity', 1);
      } else {
        $('.js-menu-container').css('opacity', 1);
        $('.js-buttons-right-container').css('opacity', 1);
      }
    };
  
    var moveToMoreDropdown = function(el) {
      el.detach().prependTo(dropdownMoreMenu.children('ul'));
    };
  
    var clearMoreDropdown = function() {
      if (dropdownMoreMenu.length > 0 && menu.length > 0) {
        dropdownMoreMenu.children('ul').children().detach().appendTo(menu);
      }
      setTimeout(wrapMenu, 0);
    };
  
    var moveRequiredLi = function() {
      var lastLi = menu.children('li').last();
  
      if (lastLi.length > 0) {
        moveToMoreDropdown(lastLi);
        setTimeout(function() {
          realWidth = logoContainer.width() + menuContainer.width() + buttonsRightContainer.width();
  
          if ((windowWidth - 10) < (realWidth)) {
            moveRequiredLi();
          } else {
            menuReveal();
          }
        }, 0);
      }
    };
  
    $(window).on('resize', _.throttle(function() {
      clearMoreDropdown();
    }, 1500));
    wrapMenu();
  
    // Open Login modal
    $('.js-login').click(function(e) {
      e.preventDefault();
      if (false) {
        window.location = $('a.js-login').eq(0).attr('href');
      } else if (true) {
        if (AlumniConnect.Helpers.Global.isMobile.any() == null) {
          $("#login-modal").foundation('reveal', 'open', { animation_speed: 0 });
        } else {
          window.location = $('a.js-login').eq(0).attr('href');
        }
      }
      $('body').css('overflow', 'scroll');
      return false;
    });
  
    var href = '';
  
    // Event login form closed
    $(document).on('close.fndtn.reveal', '[data-reveal]', function () {
      // Remove redirect to for Login/password
      if ($(this).find('.delete-on-close').length > 0) {
        $(this).find('.delete-on-close').remove();
      }
  
      // Remove redirect to for social connect
      $(this).find('.login-form__social-connect').each(function() {
        href = $(this).find('a.social-button').attr('href');
        if (href) {
          if (href.search('&redirect_to=') != -1) {
            href = href.substr(0, href.indexOf('&redirect_to='));
            $(this).find('a.social-button').attr('href', href);
          }
        }
      });
  
      // Remove redirect to for SSO button
      href = $(this).find('a.button__sso').attr('href');
      if (href) {
        if (href.search('\\?redirect_to=') != -1) {
          href = href.substr(0, href.indexOf('?redirect_to='));
          $(this).find('a.button__sso').attr('href', href);
        }
      }
    });
  })
</script>
<script>
  $(document).ready(function() {
    $("input[name='user[timezone]']").val(moment.tz.guess());
  });
  $(function() {
    var $inputs = $('#new_user input[type=email], input[type=password]');
  
    $("#new_user").submit(function(e) {
      $.each($inputs, function(k, v) {
        if ($(v).val() == "") {
          e.preventDefault();
          $(v).addClass('input-error');
        } else {
          $(v).removeClass('input-error');
        }
      })
    })
  })
</script>
<script>
  $(document).ready(function() {
    $(document).on("click", ".js-linkedin-auth", function(e) {
      if(false){
        return; //open in the same page on mobile and tablet
      }
      else {
        e.preventDefault();
        AlumniConnect.Helpers.Linkedin.openAuthWindow($(this).attr("href"));
        return false;
      }
    });
  
    $("a#linkedin-loading").fancybox({
        'hideOnContentClick': true,
        'showCloseButton'   : false,
        'overlayOpacity'    : 0.7,
        'onStart'           : window.Loader.start,
        'onClose'           : window.Loader.stop
    });
  })
</script>
<script>
  $(document).ready(function() {
    $("#login-modal").css("width", parseInt($('.login-form').css('width')));
    $(document).on('open.fndtn.reveal', '#login-modal[data-reveal]', function () {
      $('body').css('overflow', 'hidden');
    });
    $(document).on('close.fndtn.reveal', '#login-modal[data-reveal]', function () {
      $('body').css('overflow', 'auto');
    });
  
    $(document).on('click', '#loader', function(e){
      $('#fancybox-overlay').click();
      Loader.stop();
    });
  
    if (window.self !== window.top) {
      // Hide in iframe
      $('.js-social-connects').hide();
    }
  });
</script>


