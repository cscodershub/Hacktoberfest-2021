    <script type="text/javascript">
    $(function(){
        // Adding a class to the body tag
        $('body').addClass('page-template page-template-page-platform-template page-template-page-platform-template-php page page-id-14301 page-parent et_divi_builder page-platform en et-pb-theme-boomi et-db et_minified_js et_minified_css chrome push-body');
        
        // Mobile menu toggle
        $('.menu-toggle').on('click', function(){
            $('body').toggleClass('push-body-toleft');
            $('.boomi-mobile-nav').toggleClass('menu-open');
        })

        // Search onClick functions for both regular and mobile view
        $('#boomi-search-toggle-on').on('click', function(){
            if ($(window).width() >= 1150) {
                $(this).hide();
                $('#menu-mega-menu-2').hide();
                $('.boomi-search-container').show();
                $('#boomi-search-toggle-off').show();
                $('.boomi-search-bar').css("flex", "1 1 0%");
            }
            if ($(window).width() <= 1149) {
                $(this).hide();
                $('#site-navigation').hide();
                $('.boomi-search-container').show();
                $('#boomi-search-toggle-off').show();
                $('.boomi-search-bar').css("flex", "1 1 0%");   
            }
        })
        $('#boomi-search-toggle-off').on('click', function(){
            if ($(window).width() >=1150) {
                $(this).hide();
                $('#boomi-search-toggle-on').show();
                $('#menu-mega-menu-2').show();
                $('.boomi-search-container').hide();
                $('.boomi-search-bar').css("flex", "0 0 auto");
            }
            if ($(window).width() <= 1149) {
                $(this).hide();
                $('#boomi-search-toggle-on').show();
                $('#site-navigation').show();
                $('.boomi-search-container').hide();
                $('.boomi-search-bar').css("flex", "0 0 auto");
            }
        })

        // Hamburger meun
        $('.dashicons').on('click', function(e) {
          if(e) e.preventDefault();

          $('.menu-item .sub-menu.active').removeClass('active').removeAttr('style');
          $(this).closest('.menu-item').find('.sub-menu').addClass('active').css('display', 'block');
        });

        //Change the color of the View All buttons
        $('.stats.pull-right > a')
            .on('mouseenter', function(){
                $(this).css({
                    "background-color": "#ff7c66",
                    "color": "#fff",
                    "border-color": "#fff"
                });
            })
            .on('mouseleave', function(){
                $(this).css({
                    "color": "#333",
                    "background-color": "#fff",
                    "border-color": "#adadad"
                });
            })
    })
</script>
    
