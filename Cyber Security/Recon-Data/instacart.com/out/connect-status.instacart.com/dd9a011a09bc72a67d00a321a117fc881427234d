    var eventPayload = {"name":"statusPageScreen","attributes":{"pageIsPrivate":false,"overAllocationsCohort":null,"pageId":"5t2xy2vhfjsk","pageType":"public","componentSubscriptionEnabled":false,"pricingPlan":"public-business","subscriptionType":"paid","orgId":"j7kr6p7bsdt9","isAaID":false,"aaidMigration":"not available"}}
    var expectedParams = new Set(); // initializing with iterable not supported in IE11
    expectedParams.add('utm_source').add('utm_campaign').add('utm_medium');

    location.search.substring(1).split('&').forEach(function(param) {
      var arr = param.split('=');
      if (arr.length < 2) return;

      var k = arr[0], v = arr[1]
      if (expectedParams.has(k)) eventPayload.attributes[k] = v;
    });

    analyticsClient.then(function(analyticsClient) {
      analyticsClient.sendScreenEvent(eventPayload)
    })

