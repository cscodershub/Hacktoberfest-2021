</div><script type="text/javascript">
function refreshMiniCart() 
{
$('#miniCartContent').load(bmw.shops.commands.createPipelineURL('ViewCart-MiniCart', 0, ''), function() {
var $content = pageProperties.miniCartCount;
if ($content === null || isNaN($content) || $content == "0" || $content == "") {
$('#miniCart .ammount').removeClass('basketActive');
$("#miniCartCount").html("0");
} else {
$('#miniCart .ammount').addClass('basketActive');
$("#miniCartCount").html($content);
}
});
}
function addToCartNow(_trigger) 
{
if ($(_trigger).hasClass('incart'))
{
return true;
}
var preloader = bmw.shops.util.PngPreloader($(_trigger).closest('.teaserContainer').find('.ajaxPngPreloader'));
var disabled = $(_trigger).closest('.teaserContainer').find('.disabled,.wait');
$.ajax({
method: 'POST',
url: 'https://shop.bmw.de/INTERSHOP/web/WFS/BMWDE-MarketDE-Site/de_DE/-/EUR/ViewCart-AddProduct?SkipReload=true',
data: $(_trigger).closest('.teaserContainer').find('.immediateProductAdd').serialize(),
beforeSend: function(){
preloader.show();
disabled.fadeIn();}
})
.done(function( data ) {
$.each(data, function(index, element) {
if ($('.teaserCart').hasClass('teaserCart'))
{
showTeaserCart(element.CartCount,element.TotalPrice);
for(var i = 0; i < element.Items.length; i++) {
var pli = element.Items[i];
setInCart(pli.SKU);
}
}
});
preloader.hide();
disabled.fadeOut();
refreshMiniCart();
})
.fail(function(){});
}
</script><div class="teaserContainer addProduct">
<div class="shadow product teaser1line1col">
<div class="disabled"></div>
<div class="wait">
<div class="ajaxPngPreloader"></div>
