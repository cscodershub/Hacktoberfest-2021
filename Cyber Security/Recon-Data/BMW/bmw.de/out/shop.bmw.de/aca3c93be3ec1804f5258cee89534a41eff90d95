<script type="text/javascript">
function switchLocale(localeID, currentLocaleID)
{
if (bmw.shops.util.Cookie.hasAcceptCookies())
{
bmw.shops.util.Cookie.createCookie('selectedLocaleID', localeID);
}
// collect all needed variables
var $url = window.location.href;
var $productsku = '';
var $productsku_detail = '';
var $categoryname = '';
var $catalogid = '';
var $pageletentrypointid = '';
// convert url parameters into array
var currentParams = window.location.search.slice(1).split("&").map(function(n){return n = n.split("="),this[n[0]] = n[1],this}.bind({}))[0];
// create an array with celebrus tracking parameter names
var celebrusParams = ['CID', 'TYPE', 'PLA', 'CRE','tl','cid', 'type', 'pla', 'cre'];
var searchParams = ['SearchParameter'];
var count = 0, idx = 0;
// get url parameters count
for (key in currentParams){ if(key.length === 0){ continue; } if(currentParams.hasOwnProperty(key)){ count++ }}
var celebrusParamsQuery = ($url.indexOf('?') === -1 && count > 0) ? '?' : ((count > 0) ? '&' : '');
var searchParamsQuery = "&";
// check if the url contains celebrus parameters
$.each(currentParams, function(key, value){
var index = $.inArray(key, celebrusParams); idx++;
if(index != -1){ celebrusParamsQuery += (key + '=' + value + ((idx !== count) ? '&' : '')); }
});
// check if the url contains search parameter
$.each(currentParams, function(key, value){
var index = $.inArray(key, searchParams); idx++;
if(index != -1){ searchParamsQuery += (key + '=' + value); }
});
if (searchParamsQuery.length == '&') {
searchParamsQuery = '';
}
// simple pipeline switch if it isn't defined a product-, category- or content page (incl. a rewritten URL) 
if ($productsku.length == 0 && $categoryname.length == 0 && $catalogid.length == 0 && $pageletentrypointid.length == 0) {
if ($url.indexOf(currentLocaleID) == -1) {
window.location = 'https://shop.bmw.de/INTERSHOP/web/WFS/BMWDE-MarketDE-Site/de_DE/-/EUR/ViewApplication-SwitchLocale?SKU=' + $productsku + '&CategoryName=' + $categoryname + '&CatalogID=' + $catalogid + '&PEPID=' + $pageletentrypointid + '&LocaleID=' + localeID + celebrusParamsQuery;
} else {
window.location = $url.substr(0,$url.indexOf(currentLocaleID)) + localeID + $url.substr($url.indexOf(currentLocaleID) + currentLocaleID.length) + celebrusParamsQuery;
}
} else {
if ($productsku_detail.length == 0) {
window.location = 'https://shop.bmw.de/INTERSHOP/web/WFS/BMWDE-MarketDE-Site/de_DE/-/EUR/ViewApplication-SwitchLocale?SKU=' + $productsku + '&CategoryName=' + $categoryname + '&CatalogID=' + $catalogid + '&PEPID=' + $pageletentrypointid + '&LocaleID=' + localeID + celebrusParamsQuery + searchParamsQuery;
} else {
window.location = 'https://shop.bmw.de/INTERSHOP/web/WFS/BMWDE-MarketDE-Site/de_DE/-/EUR/ViewApplication-SwitchLocale?SKU=' + $productsku_detail + '&CategoryName=' + $categoryname + '&CatalogID=' + $catalogid + '&PEPID=' + $pageletentrypointid + '&LocaleID=' + localeID + celebrusParamsQuery;
}
}
};
</script><div id="cookieInfoPlaceholder"></div><script type="text/javascript">var footerCSS = document.createElement("link"); footerCSS.rel = "stylesheet"; footerCSS.href = "/INTERSHOP/static/WFS/BMWDE-MarketDE-Site/-/-/de_DE/css/styles_otv.css?lastModified=1558528026000"; footerCSS.type = "text/css"; var goDefer = document.getElementsByTagName("link")[0]; goDefer.parentNode.insertBefore(footerCSS, goDefer);</script></div>
