  <div id="main">
    <div class="content">

    <script type="text/javascript">
        
        if (window.self === window.top) {
            window.onload = function() {

                
                var DEFAULT_NEWS_URL = "https://apple.com/news";
                var url = window.location;

                var anchor = url.hash;
                if(!anchor){
                    top.location.replace(DEFAULT_NEWS_URL);
                }

                anchor = anchor.toLowerCase();
                
                if(anchor === "#au") {
                    top.location.replace("https://apple.com/au/news");
                } else
                if(anchor === "#uk") {
                    top.location.replace("https://apple.com/uk/news");
                } else{
                    top.location.replace(DEFAULT_NEWS_URL);
                }
            };
        }
    </script>
    </div>
